{
  "name": "Image Extractor Extension",
  "version": "1.1",
  "manifest_version": 3,
  "description": "Image extractor from web pages",

  "action": {
      "default_title": "image extractor",
      "default_popup": "popup.html",
      "default_icon": "yodayo.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "permissions": [
      "scripting",
      "activeTab"
  ]
}